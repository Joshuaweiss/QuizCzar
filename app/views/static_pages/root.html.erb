<section id="root" class="group"></section>


<script id="signout-button" type="template">
  <%= link_to "Sign Out", "/api/session", method: :delete, class: "signout-button" %>
</script>

<script id="logo" type="template">
  <figure data-mask="<%= asset_url("QuizCzarHorizontal.svg")%>" ></figure>
</script>

<script type="template" id="facebook-login-button">
  <%= button_to "Log in with Facebook", "/auth/facebook", class: "sign-in-facebook" %>
</script>


<script>
  $(document).ready(function(){
    var $rootEl = $("#root")
    QuizCzar.initialize($rootEl);
  });
</script>
