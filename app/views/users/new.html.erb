<form class="sign-up-form" action="<%= users_url %>" method="post" >
  <h1>Sign Up</h1>

  <label>
    Name
    <input type="string" name="user[name]">
  </label>

  <label>
    Email
    <input type="string" name="user[email]">
  </label>

  <label>
    Password
    <input type="string" name="user[password]">
  </label>

  <%= auth_token_input %>

  <button>Sign Up</button>
</form>
